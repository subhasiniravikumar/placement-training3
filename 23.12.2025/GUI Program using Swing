import javax.swing.*;

public class Main {
    public static void main(String[] args) {
        JFrame f = new JFrame("Login");
        JButton b = new JButton("Submit");
        b.setBounds(100, 100, 80, 30);

        f.add(b);
        f.setSize(300, 300);
        f.setLayout(null);
        f.setVisible(true);
    }
}
