import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter Password: ");
        String pwd = sc.next();

        if (pwd.length() >= 8)
            System.out.println("Strong Password");
        else
            System.out.println("Weak Password");
    }
}
